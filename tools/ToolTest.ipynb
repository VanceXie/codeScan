    "import numpy as np\n",
    "\n",
    "x = np.arange(6, 0, -1)\n",
    "arr1 = x[:-2]\n",
    "arr2 = x[1:-1]\n",
    "arr3 = x[2:]\n",
    "# 使用乘法操作计算每个数组的元素积\n",
    "result = arr1 * arr2 * arr3\n",
    "print(x)\n",
    "print(x[-4:])\n",
    "print(result)\n"
